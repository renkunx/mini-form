.custom-tab-bar-wrapper {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.custom-tab-bar-wrapper .text {
  font-size: 20rpx;
}

.t-tab-bar-item {
  background-color: red;
}

.custom-item {
  width: 212rpx;
  height: 212rpx;
  position: absolute;
  top: -100%;
  border-radius: 50%;
  overflow: hidden;
  z-index: 100;
  border: 1px solid;
  background: #fff;
}
